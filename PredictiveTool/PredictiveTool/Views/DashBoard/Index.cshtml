    });
    function getdate() {
        $.ajax({
            type: "GET",
            url: "/DashBoard/GetData",
            contentType: "application/json",
            dataType: "json",
            success: function (result) {
                debugger;
                //var keys = Object.keys(result);
                var cat = new Array();
                var catData = new Array();
                var Trans = [];
                var TransF = [];
                var mining = [];
                var mininF = [];
                //var totalspent = 0.0;
                for (var i = 0; i < result.length; i++)
                {
                    mining.push([result[i].year, result[i].mq]);
                    Trans.push([result[i].year, result[i].tsc]);
                    TransF.push([result[i].year, result[i].tscf]);
                    mininF.push([result[i].year, result[i].mqf]);
                    //arrLN.push(result[i].mq);
                    //totalspent += result[keys[i]];
                    //cat.push(arrL);
                    //catData.push(arrLN);
                    //catData.push(arrL, arrLN)
                }
                var asasa = catData;
                createCharts(mining, Trans, TransF, mininF);
            }
        });
    }

    function createCharts(mining, Trans, TransF, mininF) {
        Highcharts.chart('container', {

            title: {
                text: 'Strike-Mining and quarrying, 2010-2024'
            },

            subtitle: {
                text: 'Source: thesolarfoundation.com'
            },

            yAxis: {
                title: {
                    text: ''
                }
            },
            legend: {
                layout: 'vertical',
                align: 'right',
                verticalAlign: 'middle'
            },

            //plotOptions: {
            //    series: {
            //        label: {
            //            connectorAllowed: false
            //        },
            //        pointStart: 2010
            //    }
            //},

            series: [{
                name: 'Mining and Quarrying',
                data: mining
              },
                {
                    name: 'Mining and Quarrying Forecast',
                    data: mininF,
                    dashStyle: 'longdash'
                },
                {
                    name: 'Transport storage and communication',
                    data: Trans
                },
                {
                    name: 'Transport storage and communication Forecast',
                    data: TransF,
                    dashStyle: 'longdash'
                }

            ],
        });
    }


